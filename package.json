{
  "name": "@seo-booster/bookshelf-jsonapi-params",
  "version": "1.4.5",
  "description": "Automatically applies relations, filters, and more from the JSON API spec to your Bookshelf.js results",
  "main": "./dist/index.js",
  "options": {
    "mocha": "--require babel-core/register --bail test --exit"
  },
  "scripts": {
    "build": "babel -q -L -D ./src/ --out-dir ./dist/",
    "prepublish": "npm run clean && npm run build",
    "test": "mocha $npm_package_options_mocha",
    "clean": "rm -rf dist/"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:massimo-ua/bookshelf-jsonapi-params.git"
  },
  "keywords": [
    "bookshelf",
    "knex",
    "jsonapi",
    "json api",
    "jsonb",
    "jsonb filtering",
    "relationship filtering"
  ],
  "author": "James Dixon <jim.w.dixon@gmail.com> (https://github.com/jamesdixon)",
  "contributors": [
    "Alec Hirsch <alech1215@gmail.com> (https://github.com/alechirsch)"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/massimo-ua/bookshelf-jsonapi-params/issues"
  },
  "homepage": "https://github.com/massimo-ua/bookshelf-jsonapi-params",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-syntax-object-rest-spread": "^6.13.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "bluebird": "^3.7.2",
    "bookshelf": "^0.15.1",
    "chai": "^4.2.0",
    "eslint": "^4.19.1",
    "eslint-config-hapi": "^9.1.0",
    "@hapi/eslint-plugin-hapi": "^4.1.0",
    "knex": "0.17.2",
    "mocha": "^6.2.2",
    "pg": "^4.5.3",
    "sqlite3": "^4.1.1"
  },
  "engines": {
    "iojs": ">= 1.0.0",
    "node": ">= 0.10"
  },
  "dependencies": {
    "bookshelf-page": "^0.3.2",
    "inflection": "^1.12.0",
    "lodash": "^4.17.15",
    "split-string": "^4.0.0"
  },
  "directories": {
    "test": "test"
  }
}
